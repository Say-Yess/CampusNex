{
    "version": 2,
    "name": "campusnex-api",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "@database_url",
        "JWT_SECRET": "@jwt_secret",
        "GOOGLE_CLIENT_ID": "@google_client_id",
        "GOOGLE_CLIENT_SECRET": "@google_client_secret",
        "SESSION_SECRET": "@session_secret"
    },
    "functions": {
        "index.js": {
            "maxDuration": 30
        }
    }
}